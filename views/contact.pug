extends base.pug 

block scripts 
    script(src="/static/index.js") 
block style 
    style
      include ../static/style.css
      include ../static/styleContact.css
block content 
    div.container
        h1 Contact Us 
        form(action='/contact', method='post',class='myform')
            input(type='text',class ='myclass', name='name', placeholder='Enter your Name')
            input(type='gender',class ='myclass', name='gender', placeholder='Enter your Gender')
            input(type='email',class ='myclass', name='email', placeholder='Enter your Email ')
            input(type='number',class ='myclass', name='number', placeholder='Enter your Phone NO.')
            input(type='text',class ='myclass', name='dance', placeholder='Enter your dance form')
            button.btn SUMBIT
